{
  "index_patterns": ["clin-qa-sequencings*", "clin-staging-sequencings*",  "clin-prod-sequencings*"],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "analysis": {
        "normalizer": {
          "lowercase_normalizer": {
            "type": "custom",
            "char_filter": [],
            "filter": "lowercase"
          }
        }
      }
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "request_id": {
          "type": "keyword",
          "normalizer" : "lowercase_normalizer"
        },
        "patient_id": {
          "type": "keyword",
          "normalizer" : "lowercase_normalizer"
        },
        "patient_mrn": {
          "type": "keyword",
          "normalizer" : "lowercase_normalizer"
        },
        "status": {
          "type": "keyword"
        },
        "priority": {
          "type": "keyword"
        },
        "prenatal": {
          "type": "boolean"
        },
        "analysis_code": {
          "type": "keyword"
        },
        "requester": {
          "type": "keyword"
        },
        "ldm": {
          "type": "keyword"
        },
        "ep": {
          "type": "keyword"
        },
        "created_on": {
          "type": "keyword"
        },
        "timestamp": {
          "type": "date"
        },
        "security_tags": {
          "type": "keyword"
        },
        "prescription_id": {
          "type": "keyword",
          "normalizer" : "lowercase_normalizer"
        },
        "prescription_status": {
          "type": "keyword"
        },
        "sample": {
          "type": "keyword",
          "normalizer" : "lowercase_normalizer"
        }
      }
    }
  }
}